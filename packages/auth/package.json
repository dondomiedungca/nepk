{
  "name": "@nepk/auth",
  "version": "1.0.0",
  "description": "General purpose for implementation of auth module.",
  "author": "",
  "license": "UNLICENSED",
  "main": "dist/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run compile && yarn run clean",
    "clean": "rimraf --glob ./tsconfig.tsbuildinfo",
    "compile": "yarn tsc -b tsconfig.json",
    "watch": "yarn run compile --watch --preserveWatchOutput && yarn run clean",
    "migration:up": "yarn run compile && yarn run clean && typeorm -d dist/datasource.js migration:run"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.0.0",
    "class-transformer": "^0.5.1",
    "mongoose": "^7.5.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/node": "^20.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src"
  }
}
